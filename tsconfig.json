{
  "compilerOptions": {
    "outDir": "./dist/",
    "allowJs": true,
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "strict": true,
    "module": "commonjs",
  },
  "include": ["./src/**/*"],
  "allowSyntheticDefaultImports" : true,
  "esModuleInterop" : true
}
